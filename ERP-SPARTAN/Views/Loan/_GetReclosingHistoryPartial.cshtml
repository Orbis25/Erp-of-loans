@model IEnumerable<Models.Models.HiLoans.ReclosingHistory>
<div class="col-12">
    <div class="table-responsive">
        <table class="table table-bordered mt-4" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>Monto</th>
                    <th>Fecha</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Amount</td>
                        <td>@item.CreatedAtStr</td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
</div>